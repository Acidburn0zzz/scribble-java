module demo.bettybook.math.Math;

type <java> "java.lang.Integer" from "rt.jar" as Int;


// N.B. calling this Math will clash with java.lang.Math
global protocol MathService(role C, role S)
{
	choice at C
	{
		(Int) from C to S;
		choice at C
		{
			Add(Int) from C to S;
			Sum(Int) from S to C;
		}
		or
		{
			Neg() from C to S;
			Neg(Int) from S to C;
		}
		do MathService(C, S);
	}
	or
	{
		Bye() from C to S;
		Bye() from S to C;
	}
}
