/**
 * Assuming scribblec.sh in scribble-java root directory:
 * 
 * ./scribblec.sh -d modules/demos/scrib/betty16/src modules/demos/scrib/betty16/src/demo/bettybook/http/shortv/HttpShort.scr -api Http C
 */


module demo.bettybook.http.shortv.HttpShort;


sig <java> "demo.bettybook.http.shortv.message.client.Req"
		from "betty16/httpshort/message/Req.java"
		as Req;

sig <java> "demo.bettybook.http.shortv.message.server.Resp"
		from "betty16/shortvers/message/Resp.java"
		as Resp;


global protocol Http(role C, role S) {
	Req from C to S;
	Resp from S to C;
}
