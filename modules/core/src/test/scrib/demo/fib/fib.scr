module demo.fib;

type <java> "java.lang.Integer" from "rt.jar" as Integer;

global protocol Adder(role AddClient, role AddServer)
{
	rec X
	{
		choice at AddClient
		{
			ADD(Integer, Integer) from AddClient to AddServer;
			RES(Integer) from AddServer to AddClient;
			continue X;
		}
		or
		{
			BYE() from AddClient to AddServer;
		}
	}
}
