module scribble.examples.GProtocolInstance;

type <xsd> "{http://www.acme.com/financial}Order" from "http://www.acme.com/schemas/Order.xsd" as OrderRequest;

local protocol Submit at Source<sig Op as SubmitOp>(role Source,role Destination) {
}

local protocol Submit at Destination<sig Op as SubmitOp>(role Source,role Destination) {
}

local protocol PlaceOrder at Buyer(role Buyer,role Seller) instantiates Submit<place(OrderRequest)>(Buyer,Seller);

local protocol PlaceOrder at Seller(role Buyer,role Seller) instantiates Submit<place(OrderRequest)>(Buyer,Seller);
