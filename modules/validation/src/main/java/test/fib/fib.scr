module test.fib;

type <java> "java.lang.Integer" from "rt.jar" as Integer;

global protocol Adder(role A, role B)
{
	rec X
	{
		choice at A
		{
			ADD(Integer, Integer) from A to B;
			RES(Integer) from B to A;
			continue X;
		}
		or
		{
			BYE() from A to B;
		}
	}
}
