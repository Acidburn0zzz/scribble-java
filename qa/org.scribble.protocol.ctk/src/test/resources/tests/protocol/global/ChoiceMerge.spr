import Order;
import Cancel;
import Invoice;
import Cancelled;

protocol ChoiceMerge(role Buyer) {
	Buyer introduces Broker;
	Broker introduces Seller;

	choice at Buyer {
		Order from Buyer to Broker;
		Order from Broker to Seller;
		Invoice from Seller to Broker;
		Invoice from Broker to Buyer;
	} or {
		Cancel from Buyer to Broker;
		Cancel from Broker to Seller;
		Cancelled from Seller to Broker;
		Cancelled from Broker to Buyer;
	}
}
