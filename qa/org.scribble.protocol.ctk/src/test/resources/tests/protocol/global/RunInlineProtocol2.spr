import Order;
import Confirm;
import CreditCheck;
import CreditRating;

protocol RunInlineProtocol2 {
	role Buyer, Seller;

	Order from Buyer to Seller;
	
	run protocol (Seller) {
		role CreditAgency;
		
		CreditCheck from Seller to CreditAgency;
		CreditRating from CreditAgency to Seller;
	}
	
	Confirm from Seller to Buyer;
}
