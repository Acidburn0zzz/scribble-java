import Order;
import Confirm;
import CreditCheck;
import CreditRating;

protocol RunInlineProtocol2 (role Buyer) {
	Buyer introduces Seller;

	Order from Buyer to Seller;
	
	run protocol (Seller) {
		Seller introduces CreditAgency;
		
		CreditCheck from Seller to CreditAgency;
		CreditRating from CreditAgency to Seller;
	}
	
	Confirm from Seller to Buyer;
}
