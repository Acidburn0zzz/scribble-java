import Order;
import Finish;
import Rejected;
import Accepted;
import Another;

protocol Recur (role Buyer) {
	Buyer introduces Seller;

	rec Transaction {
		Order from Buyer to Seller;
		
		choice at Seller {
			Transaction;
		} or {
		}
		
		Another from Buyer to Seller;
	}
	
	Finish from Buyer to Seller;
}
