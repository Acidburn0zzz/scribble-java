import Order;
import OutOfStock;
import OrderExpired, OrderCancelled;
import Confirm;
import Ack;
import Finish;

protocol DoInterrupt@Buyer {
	Buyer introduces Seller;

	do {
		Order to Seller;
		Confirm from Seller;
	} interrupt {
		OutOfStock from Seller;
	} interrupt {
		expire(OrderExpired) to Seller;
		OrderCancelled to Seller;
		Ack from Seller;
	}
	
	Finish to Seller;
}
