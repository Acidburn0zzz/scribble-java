import Order;
import Confirmation;
import OutOfStock;
import InvalidProduct;

protocol MultiPartyInteractionsAndChoice2 at Buyer {
	Buyer introduces Broker;

	Order to Broker;
	
	choice {
		Confirmation from Broker;
	} or {
		OutOfStock from Broker;
	} or {
		invalidProduct(InvalidProduct) from Broker;
	}
}
