import Order;
import CreditCheck;
import CreditOk;
import Confirmation;
import OutOfStock;
import InsufficientCredit;

protocol MultiPartyInteractionsAndChoice@Seller {
	Broker introduces Seller;

	Order from Broker;

	choice at Seller {
		Confirmation to Broker;
	} or {
		OutOfStock to Broker;
	}
}
