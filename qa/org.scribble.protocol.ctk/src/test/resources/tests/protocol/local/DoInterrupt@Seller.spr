import Order;
import OutOfStock;
import OrderExpired, OrderCancelled;
import Confirm;
import Ack;
import Finish;

protocol DoInterrupt at Seller (role Buyer) {

	do {
		Order from Buyer;
		Confirm to Buyer;
	} interrupt {
		OutOfStock to Buyer;
	} interrupt {
		expire(OrderExpired) from Buyer;
		OrderCancelled from Buyer;
		Ack to Buyer;
	}
	
	Finish from Buyer;
}
