import Order;
import Finish;
import Rejected;
import Accepted;
import Another;

protocol Recur@Seller {
	Buyer introduces Seller;

	rec Transaction {
		Order from Buyer;
		
		choice at Seller {
			tryAgain() to Buyer;
			Transaction;
		} or {
			exit() to Buyer;
		}
		
		Another from Buyer;
	}
	
	Finish from Buyer;
}
