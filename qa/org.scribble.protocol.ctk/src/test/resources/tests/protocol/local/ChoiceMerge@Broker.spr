import Order;
import Cancel;
import Invoice;
import Cancelled;

protocol ChoiceMerge@Broker (role Buyer) {
	Broker introduces Seller;

	choice {
		Order from Buyer;
		Order to Seller;
		Invoice from Seller;
		Invoice to Buyer;
	} or {
		Cancel from Buyer;
		Cancel to Seller;
		Cancelled from Seller;
		Cancelled to Buyer;
	}
}
